{
  "name": "vscode-FSToolbox",
  "displayName": "FSToolbox",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-FSToolbox.CopyFilePath",
        "title": "FST: Copy active file path"
      },
      {
        "command": "vscode-FSToolbox.CopyFileName",
        "title": "FST: Copy active file name"
      },
      {
        "command": "vscode-FSToolbox.CopyRelativeFilePath",
        "title": "FST: Copy relative file path"
      },
      {
        "command": "vscode-FSToolbox.CopyFileNameWithoutExtension",
        "title": "FST: Copy active file name without extension"
      },
      {
        "command": "vscode-FSToolbox.CopyFilePathWithLineNumber",
        "title": "FST: Copy active file path name and line number"
      },
      {
        "command": "vscode-FSToolbox.CopyFileNameWithLineNumber",
        "title": "FST: Copy active file name and line number"
      },
      {
        "command": "vscode-FSToolbox.CopyWorkspaceRootPath",
        "title": "FST: Copy Workspace root path"
      },
      {
        "command": "vscode-FSToolbox.CopyFolderPath",
        "title": "FST: Copy folder path"
      },
      {
        "command": "vscode-FSToolbox.CopyRelativeFilePath",
        "title": "FST: Copy relative file path"
      },
      {
        "command": "vscode-FSToolbox.CopyRelativeFilePathWithLineNumber",
        "title": "FST: Copy relative file path with line number"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "ts-loader": "^8.0.18",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {}
}
